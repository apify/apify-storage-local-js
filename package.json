{
    "name": "@apify/storage-local",
    "version": "1.1.0",
    "description": "Drop in replacement of Apify API with a local SQLite and filesystem implementation. Not all API features are supported.",
    "engines": {
        "node": ">=10.17.0"
    },
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.mjs",
            "require": "./dist/index.js"
        }
    },
    "keywords": [
        "apify",
        "sqlite",
        "api"
    ],
    "author": {
        "name": "Apify",
        "email": "support@apify.com",
        "url": "https://apify.com"
    },
    "contributors": [
        "Ondra Urban <ondra@apify.com>"
    ],
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/apify/apify-storage-local-js"
    },
    "bugs": {
        "url": "https://github.com/apifytech/apify-client-local/issues"
    },
    "homepage": "https://sdk.apify.com/",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsc -b src",
        "postbuild": "gen-esm-wrapper dist/index.js dist/index.mjs",
        "prepublishOnly": "npm run build",
        "clean": "tsc -b src --clean",
        "test": "jest",
        "lint": "eslint src test --ext js,mjs,ts",
        "lint:fix": "eslint src test --ext js,mjs,ts --fix",
        "build:watch": "tsc -b src -w"
    },
    "dependencies": {
        "apify-shared": "^0.6.3",
        "better-sqlite3-with-prebuilds": "7.1.5",
        "content-type": "^1.0.4",
        "fs-extra": "^9.1.0",
        "mime-types": "^2.1.29",
        "ow": "^0.23.0",
        "tslib": "^1.14.1"
    },
    "devDependencies": {
        "@apify/eslint-config-ts": "^0.1.1",
        "@types/better-sqlite3": "^5.4.1",
        "@types/fs-extra": "^9.0.8",
        "@types/jest": "^26.0.21",
        "@types/node": "^14.14.35",
        "@typescript-eslint/eslint-plugin": "^4.18.0",
        "@typescript-eslint/parser": "^4.18.0",
        "eslint": "^7.22.0",
        "gen-esm-wrapper": "^1.1.1",
        "jest": "^26.6.3",
        "jest-circus": "^26.6.3",
        "ts-jest": "^26.5.4",
        "ts-node": "^9.1.1",
        "typescript": "^4.2.3"
    }
}
